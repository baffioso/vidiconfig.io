    <div id="side-panel">
        <div role="tabpanel">
            <!-- Nav tabs -->
            <ul class="nav nav-tabs" role="tablist" id="main-tabs">
                <li role="presentation" class="active"><a href="#search-content" aria-controls="search-content"
                                                          role="tab" data-toggle="tab">{{Search}}</a></li>
                <li role="presentation"><a href="#info-content" aria-controls="info-content" role="tab"
                                           data-toggle="tab">{{Info}}</a></li>
                <li role="presentation"><a href="#layer-content" aria-controls="" role="tab"
                                           data-toggle="tab">{{Layers}}</a></li>
                <li role="presentation"><a href="#baselayer-content" aria-controls="" role="tab"
                                           data-toggle="tab">{{Baselayers}}</a></li>
                <li role="presentation"><a href="#legend-content" aria-controls="" role="tab"
                                           data-toggle="tab">{{Legend}}</a></li>
                <li role="presentation"><a href="#draw-content" aria-controls="" role="tab"
                                           data-toggle="tab">{{Draw}}</a></li>
                <li role="presentation"><a href="#print-content" aria-controls="" role="tab"
                                           data-toggle="tab">{{Print}}</a></li>
            </ul>
            <!-- Tab panes -->
            <div class="tab-content main-content">
                <div role="tabpanel" class="tab-pane active" id="search-content">
                    <div id="place-search">
                        <div class="alert alert-dismissible alert-info" role="alert">
                            <button type="button" class="close" data-dismiss="alert">×</button>
                            {{Search places}}
                        </div>
                        <div class="places">
                            <div class="btn-group">
                                <input id="custom-search" class="custom-search typeahead" type="text"
                                       placeholder="{{Address}}">
                                <span id="searchclear" class="glyphicon glyphicon-remove-circle"></span>
                            </div>
                        </div>
                    </div>
                </div>
                <div role="tabpanel" class="tab-pane fade" id="info-content">
                    <div class="alert alert-dismissible alert-info" role="alert">
                        <button type="button" class="close" data-dismiss="alert">×</button>
                        {{{Info text}}}
                    </div>

                    <div class="panel panel-default">
                        <div class="panel-body">
                            <div class="togglebutton">
                                <label>
                                    <input id="advanced-info-btn" type="checkbox"> {{Activate advanced query}}
                                </label>
                            </div>
                            <div id="buffer" style="display: none">
                                <div>
                                    <label for="buffer-value" class="control-label">{{Buffer}}</label>
                                    <input id="buffer-value" class="form-control">
                                    <div id="buffer-slider" class="slider shor"></div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div id="info-box">
                        <div id="modal-info-body">
                            <ul class="nav nav-tabs" id="info-tab"></ul>
                            <div class="tab-content" id="info-pane"></div>
                        </div>
                    </div>
                    <table id="dtable" data-show-toggle="true"></table>

                </div>
                <div role="tabpanel" class="tab-pane fade" id="layer-content">
                    <div class="alert alert-dismissible alert-info" role="alert">
                        <button type="button" class="close" data-dismiss="alert">×</button>
                        {{Help text overlays}}
                    </div>


                    <div class="form-group" id="layers" role="tablist"></div>
                </div>
                <div role="tabpanel" class="tab-pane fade" id="baselayer-content">
                    <div class="alert alert-dismissible alert-info" role="alert">
                        <button type="button" class="close" data-dismiss="alert">×</button>
                        {{Help text baselayers}}
                    </div>
                    <div class="panel panel-default">
                        <div class="panel-body">
                            <div class="form-group" id="base-layer-list"></div>
                        </div>
                    </div>
                </div>
                <div role="tabpanel" class="tab-pane fade" id="legend-content">
                    <div class="alert alert-dismissible alert-info" role="alert">
                        <button type="button" class="close" data-dismiss="alert">×</button>
                        {{Help text legend}}
                    </div>
                    <div class="panel panel-default">
                        <div class="panel-body">
                            <div id="legend" class="legend"></div>

                        </div>
                    </div>
                </div>
                <div role="tabpanel" class="tab-pane fade" id="draw-content">
                    <div class="alert alert-dismissible alert-info" role="alert">
                        <button type="button" class="close" data-dismiss="alert">×</button>
                        {{Help text draw}}
                    </div>
                    <div class="panel panel-default" id="draw-panel">
                        <div class="panel-body">
                            <div class="togglebutton">
                                <label>
                                    <input id="draw-btn" type="checkbox"> {{Activate drawing tools}}
                                </label>
                            </div>


                            <div class="panel panel-default">
                                <div class="panel-heading" role="tab"><h4 class="panel-title"><a style="display: block"
                                                                                                 class="accordion-toggle"
                                                                                                 data-toggle="collapse"
                                                                                                 href="#draw-collapse"><i
                                        class="material-icons">&#xE8B8;</i> {{Line style}}</a></h4></div>
                                <div id="draw-collapse" class="accordion-body collapse">

                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div style="margin: 15px 0 0 10px "><h4>{{{Settings for all new drawings}}}</h4></div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-lg-6">

                                            <label for="draw-colorpicker-input"
                                                   class="col-md-3 control-label">{{Color}}</label>

                                            <div id="draw-colorpicker"
                                                 class="input-group colorpicker-component col-md-10">
                                                <input id="draw-colorpicker-input" name="draw-colorpicker-input"
                                                       type="text" value="#ff0000" class="form-control"
                                                       style="margin-left: 15px;"/>
                                                <span class="input-group-addon"><i
                                                        style="margin-left: 10px;"></i></span>
                                            </div>

                                        </div>

                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <label for="draw-line-type"
                                                       class="col-md-3 control-label">{{Type}}</label>

                                                <div class="col-md-10">
                                                    <select id="draw-line-type" name="draw-line-type"
                                                            class="form-control">
                                                        <option value="none">{{Solid}}</option>
                                                        <option value="7, 7">{{Dashed, short}}</option>
                                                        <option value="12, 7">{{Dashed, medium}}</option>
                                                        <option value="17, 7">{{Dashed, long}}</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>

                                    </div>

                                    <div class="row">

                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <label for="draw-line-weight"
                                                       class="col-md-3 control-label">{{Weight}}</label>

                                                <div class="col-md-10">
                                                    <select id="draw-line-weight" name="draw-line-weight"
                                                            class="form-control">
                                                        <option value="2">{{X-thin}}</option>
                                                        <option value="3">{{Thin}}</option>
                                                        <option value="4" selected>{{Medium}}</option>
                                                        <option value="5">{{Thick}}</option>
                                                        <option value="6">{{X-Thick}}</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <label for="draw-line-type"
                                                       class="col-md-3 control-label">{{Cap}}</label>

                                                <div class="col-md-10">
                                                    <select id="draw-line-cap" name="draw-line-cap"
                                                            class="form-control">
                                                        <option value="butt">{{Butt}}</option>
                                                        <option value="round">{{Round}}</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>

                                    </div>

                                    <div class="row">
                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <label for="draw-measure"
                                                       class="col-md-3 control-label">{{Measure}}</label>

                                                <div class="col-md-10">
                                                    <div class="checkbox">
                                                        <label style="margin-top: 5px">
                                                            <input id="draw-measure" name="draw-measure"
                                                                   type="checkbox">
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div style="margin: 15px 0 0 10px "><h4>{{{Settings for new lines only}}}</h4></div>
                                        </div>
                                    </div>
                                    <div class="row">

                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <label for="draw-line-extremity"
                                                       class="col-md-3 control-label">{{Extremities}}</label>

                                                <div class="col-md-10">
                                                    <select id="draw-line-extremity" name="draw-line-extremity"
                                                            class="form-control">
                                                        <option value="none">{{None}}</option>
                                                        <option value="arrowM">{{Arrow}}</option>
                                                        <option value="stopM">{{Stop}}</option>
                                                        <option value="squareM">{{Square}}</option>
                                                        <option value="dotM">{{Dot}}</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <label for="draw-line-extremity-size"
                                                       class="col-md-3 control-label">{{Extremity size}}</label>

                                                <div class="col-md-10">
                                                    <select id="draw-line-extremity-size"
                                                            name="draw-line-extremity-size" class="form-control"
                                                            disabled>
                                                        <option value="2">{{X-Small}}</option>
                                                        <option value="3">{{Small}}</option>
                                                        <option value="4" selected>{{Medium}}</option>
                                                        <option value="5">{{Large}}</option>
                                                        <option value="6">{{X-Large}}</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>

                                    </div>


                                    <div class="row">

                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <label for="draw-line-extremity-where"
                                                       class="col-md-3 control-label">{{Extremity ends}}</label>

                                                <div class="col-md-10">
                                                    <select id="draw-line-extremity-where"
                                                            name="draw-line-extremity-where" class="form-control"
                                                            disabled>
                                                        <option value="1">{{End}}</option>
                                                        <option value="2">{{Start}}</option>
                                                        <option value="3">{{Both}}</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <label for="draw-line-total-dist" class="col-md-3 control-label">{{Total distance}}</label>

                                                <div class="col-md-10">
                                                    <div class="checkbox">
                                                        <label style="margin-top: 5px">
                                                            <input id="draw-line-total-dist" name="draw-line-total-dist"
                                                                   type="checkbox" disabled>
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    </div>

                                </div>
                            </div>


                            <div id="draw-table"></div>
                        </div>
                    </div>
                </div>
                <div role="tabpanel" class="tab-pane fade" id="print-content">
                    <div class="alert alert-dismissible alert-info" role="alert">
                        <button type="button" class="close" data-dismiss="alert">×</button>
                        {{Help text print}}
                    </div>
                    <div class="panel panel-default">
                        <div class="panel-body">
                            <div class="togglebutton">
                                <label>
                                    <input id="print-btn" type="checkbox"> {{Activate print tools}}
                                </label>
                            </div>
                            <div class="btn-toolbar bs-component" style="margin: 0;">
                                <div class="btn-group">
                                    <button id="start-print-btn" class="btn btn-raised" style="width: 130px"
                                            data-loading-text="<i class='fa fa-cog fa-spin fa-lg'></i> {{Make PDF}}
                                    " disabled><i class='fa fa-cog fa-lg'></i> {{Make PDF}}
                                    </button>
                                </div>
                                <fieldset disabled id="get-print-fieldset">
                                    <div class="btn-group">
                                        <a target="_blank" href="javascript:void(0)" class="btn btn-primary btn-raised"
                                           id="open-pdf">{{Open PDF}}</a>
                                        <a href="bootstrap-elements.html" data-target="#"
                                           class="btn btn-primary btn-raised dropdown-toggle"
                                           data-toggle="dropdown"><span class="caret"></span></a>
                                        <ul class="dropdown-menu">
                                            <li><a href="javascript:void(0)" id="download-pdf">{{Download}}</a></li>
                                            <li><a target="_blank" href="javascript:void(0)" id="open-html">{{Open HTML page}}</a>
                                            </li>
                                        </ul>
                                    </div>
                                </fieldset>
                            </div>
                            <div>
                                <form class="form-horizontal" id="print-form" role="form" data-toggle="validator">
                                    <fieldset>
                                        <div class="form-group">
                                            <label for="select-scale" class="col-md-2 control-label">{{Scale}}</label>
                                            <div class="col-md-10">
                                                <div class="input-group">
                                                    <div class="input-group-btn">
                                                        <button type="button" class="btn btn-default dropdown-toggle"
                                                                data-toggle="dropdown" aria-haspopup="true"
                                                                aria-expanded="false" disabled>1: <span
                                                                class="caret"></span></button>
                                                        <ul id="ul-scale"
                                                            class="dropdown-menu dropdown-menu-right"></ul>
                                                    </div>
                                                    <input type="number" min="200" id="select-scale"
                                                           class="form-control" disabled>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="form-group" style="display: none">
                                            <label class="col-md-2 control-label">{{Template}}</label>
                                            <div class="col-md-10" id="print-tmpl"></div>
                                        </div>

                                        <div class="form-group">
                                            <label class="col-md-2 control-label">{{Page size}}</label>
                                            <div class="col-md-10" id="print-size"></div>
                                        </div>

                                        <div class="form-group">
                                            <label class="col-md-2 control-label">{{Orientation}}</label>
                                            <div class="col-md-10" id="print-orientation"></div>
                                        </div>

                                        <div class="form-group">
                                            <label for="print-title" class="col-md-2 control-label">{{Title}}</label>

                                            <div class="col-md-10">
                                                <input type="text" class="form-control" id="print-title" disabled>
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <label for="print-comment"
                                                   class="col-md-2 control-label">{{Comment}}</label>
                                            <div class="col-md-10">
                                                <textarea class="form-control" rows="3" id="print-comment"
                                                          disabled></textarea>
                                                <span class="help-block">{{The comment will be placed on the PDF}}</span>
                                            </div>
                                        </div>

                                        <div class="form-group">
                                            <div class="togglebutton">
                                                <label>
                                                    <input id="add-legend-btn" type="checkbox" disabled> {{Show legend on print}}
                                                </label>
                                            </div>
                                        </div>

                                    </fieldset>
                                </form>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</nav>

